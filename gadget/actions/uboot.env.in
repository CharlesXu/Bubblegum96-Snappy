arch=arm
baudrate=115200
board=hikey
board_name=hikey
bootcmd=run snappy_boot
bootdelay=0
cpu=armv8
kernel_file=vmlinuz
fdtfile=s900_96board.dtb
initrd_file=initrd.img
kernel_addr_r=0x00080000
fdt_addr_r=0x10000000
ramdisk_addr=0x1ffffc0
loadfiles=run loadkernel; run loadfdt; run loadinitrd
loadkernel=fatload mmc 0:1 ${kernel_addr_r} ${snappy_kernel}/${kernel_file}
loadfdt=fatload mmc 0:1 ${fdt_addr_r} ${snappy_kernel}/dtbs/${fdtfile}
loadinitrd=fatload mmc 0:1 ${ramdisk_addr} ${snappy_kernel}/${initrd_file}; setenv initrd_size ${filesize}
mmcargs=setenv bootargs "console=ttyS5,115200n8 root=${mmcroot}"
snappy_ab=a
snappy_boot=if test "${snappy_mode}" = "try"; then if test "${snappy_trial_boot}" = "1"; then setenv snappy_os "${snappy_good_os}"; setenv snappy_kernel "${snappy_good_k
ernel}"; saveenv; else setenv snappy_trial_boot 1; saveenv; fi; fi; run loadfiles; setenv mmcroot "/dev/disk/by-label/writable ${snappy_cmdline} snappy_os=${snappy_os} s
nappy_kernel=${snappy_kernel}"; run mmcargs; booti ${kernel_addr_r} ${ramdisk_addr}:${initrd_size} ${fdt_addr_r}
snappy_cmdline=net.ifnames=0 init=/lib/systemd/systemd ro panic=-1 fixrtc
snappy_mode=regular
snappy_trial_boot=0
stderr=serial
stdin=serial
stdout=serial
vendor=hisilicon
